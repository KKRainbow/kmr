{
  "mapBucket": {
    "bucketType": "filesystem",
    "config": {
      "directory": "/"
    }
  },
  "interBucket": {
    "bucketType": "filesystem",
    "config": {
      "directory": "/tmp/wc/"
    }
  },
  "reduceBucket": {
    "bucketType": "filesystem",
    "config": {
      "directory": "/tmp/wc/"
    }
  },
  "nWorker": 1,
  "image": "kmr",
  "command": [
    "go",
    "run",
    "/go/src/github.com/naturali/kmr/examples/wordcount/main.go"
  ],
  "map": {
    "readerType": "textstream",
    "objects": [
      "/etc/passwd"
    ]
  },
  "reduce": {
    "nReduce": 1
  }
}
